{
    "name": "HistoryManager",
    "version": "1.2",
    "manifest_version": 3,


    "icons": {"128": "img/snoopy_coffee.png"},

    "description": "HistoryManager displays URL.",

    "action": {
        "default_title": "HistoryManager",
        "default_popup": "mostUseURL.html"
    },

    "permissions": [
      "history",
      "topSites"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "web_accessible_resources":[{
      "resources": [
        "dic/dic.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }],
    "content_security_policy":  {
      "sandbox": "sandbox allow-scripts; script-src 'self' 'unsafe-eval'; worker-src blob:"
    },
    "author": "fukumitsu"

}
